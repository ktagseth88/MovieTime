@{
    ViewData["Title"] = "Home Page";
}

<form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="UploadWatchList">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload your imdb list</p>
            <input type="file" name="watchlistEntry">
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload">
        </div>
    </div>
</form>